keywords = {Amino Acid Sequence,DNA,Haloarcula marismortui,Haloarcula marismortui: genetics,Helix-Loop-Helix Motifs,Models, Molecular,Molecular Sequence Data,Nucleic Acid Conformation,RNA, Archaeal,RNA, Archaeal: chemistry,RNA, Archaeal: metabolism,RNA, Ribosomal,RNA, Ribosomal: chemistry,RNA, Ribosomal: metabolism,Ribosomal Proteins,Ribosomal Proteins: metabolism},
month = aug,
number = {15},
pages = {4214--21},
pmid = {11483524},
title = {{The kink-turn: a new RNA secondary structure motif.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=149158\&tool=pmcentrez\&rendertype=abstract},
