issn = {1466-5026},
journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {Actinomycetales,Actinomycetales: classification,Actinomycetales: genetics,Actinomycetales: isolation \& purification,Actinomycetales: physiology,Aerobiosis,Anaerobiosis,Arctic Regions,Cell Wall,Cell Wall: chemistry,Cluster Analysis,DNA, Bacterial,DNA, Bacterial: chemistry,DNA, Bacterial: genetics,DNA, Ribosomal,DNA, Ribosomal: chemistry,DNA, Ribosomal: genetics,Fatty Acids,Fatty Acids: analysis,Molecular Sequence Data,Norway,Nucleic Acid Hybridization,Peptidoglycan,Peptidoglycan: analysis,Phylogeny,Pigments, Biological,Pigments, Biological: biosynthesis,Quinones,Quinones: analysis,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Sequence Analysis, DNA,Sequence Homology, Nucleic Acid,Soil Microbiology},
