doi = {10.1016/j.ejps.2012.10.014},
file = {:Users/bsweene/Documents/Papers/European journal of pharmaceutical sciences official journal of the European Federation for Pharmaceutical Sciences/2013/Therapeutic RNA aptamers in clinical trials.pdf:pdf},
