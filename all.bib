keywords = {Biophysical Phenomena,Biophysics,Capsid,Capsid: physiology,Capsid: ultrastructure,Models, Biological,Monte Carlo Method,Thermodynamics,Virus Assembly,Virus Physiological Phenomena,Viruses,Viruses: ultrastructure},
