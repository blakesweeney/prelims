journal = {Human mutation},
keywords = {Base Sequence,Humans,Mutation,RNA Splicing,RNA, Messenger},
month = jan,
number = {3},
pages = {153--71},
pmid = {9711873},
title = {{Information analysis of human splice site mutations.}},
