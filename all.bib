journal = {Journal of the American Chemical Society},
keywords = {Azoarcus,Azoarcus: metabolism,Electrophoresis, Polyacrylamide Gel,Hydroxyl Radical,Models, Chemical,Mutagenesis, Site-Directed,Mutation,Nucleic Acid Conformation,Nucleic Acid Denaturation,Nucleotides,Nucleotides: chemistry,RNA,RNA, Catalytic,RNA, Catalytic: chemistry,RNA: chemistry,Ribonuclease P,Ribonuclease P: chemistry,Ribonuclease T1,Ribonuclease T1: chemistry,Ribonucleases,Ribonucleases: chemistry,Thermodynamics},
month = jan,
number = {4},
pages = {1296--303},
pmid = {18179212},
title = {{Tertiary interactions determine the accuracy of RNA folding.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2526051\&tool=pmcentrez\&rendertype=abstract},
volume = {130},
