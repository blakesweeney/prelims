file = {:Users/bsweene/Documents/Papers/Nucleic Acids Research/2010/Cleavage of the sarcin-ricin loop of 23S rRNA differentially affects EF-G and EF-Tu binding.pdf:pdf},
issn = {1362-4962},
journal = {Nucleic Acids Research},
keywords = {Endoribonucleases,Endoribonucleases: metabolism,Escherichia coli,Escherichia coli: genetics,Fungal Proteins,Fungal Proteins: metabolism,Nucleic Acid Conformation,Peptide Elongation Factor G,Peptide Elongation Factor G: chemistry,Peptide Elongation Factor G: metabolism,Peptide Elongation Factor Tu,Peptide Elongation Factor Tu: chemistry,Peptide Elongation Factor Tu: metabolism,Protein Biosynthesis,Protein Synthesis Inhibitors,Protein Synthesis Inhibitors: metabolism,RNA, Ribosomal, 23S,RNA, Ribosomal, 23S: chemistry,RNA, Ribosomal, 23S: metabolism,Ribosomes,Ribosomes: chemistry,Ribosomes: metabolism,Ricin,Ricin: metabolism},
