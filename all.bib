pages = {W82--7},
pmid = {22618877},
title = {{TaxMan: a server to trim rRNA reference databases and inspect taxonomic coverage.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3394339\&tool=pmcentrez\&rendertype=abstract},
volume = {40},
year = {2012}
}
