@article{Win2008,
abstract = {The engineering of biological systems is anticipated to provide effective solutions to challenges that include energy and food production, environmental quality, and health and medicine. Our ability to transmit information to and from living systems, and to process and act on information inside cells, is critical to advancing the scale and complexity at which we can engineer, manipulate, and probe biological systems. We developed a general approach for assembling RNA devices that can execute higher-order cellular information processing operations from standard components. The engineered devices can function as logic gates (AND, NOR, NAND, or OR gates) and signal filters, and exhibit cooperativity. RNA devices process and transmit molecular inputs to targeted protein outputs, linking computation to gene expression and thus the potential to control cellular function.},
author = {Win, Maung Nyan and Smolke, Christina D},
doi = {10.1126/science.1160311},
file = {:Users/bsweene/Documents/Papers/Science/2008/Higher-order cellular information processing with synthetic RNA devices.pdf:pdf},
issn = {1095-9203},
journal = {Science},
keywords = {3' Untranslated Regions,Aptamers, Nucleotide,Aptamers, Nucleotide: chemistry,Aptamers, Nucleotide: metabolism,Cell Physiological Phenomena,Computers, Molecular,Gene Expression,Nucleic Acid Conformation,RNA, Catalytic,RNA, Catalytic: chemistry,RNA, Catalytic: metabolism,Saccharomyces cerevisiae,Saccharomyces cerevisiae: metabolism,Tetracycline,Tetracycline: metabolism,Theophylline,Theophylline: pharmacology},
month = oct,
number = {5900},
pages = {456--60},
pmid = {18927397},
title = {{Higher-order cellular information processing with synthetic RNA devices.}},
