issn = {1469-9001},
journal = {RNA},
month = jul,
number = {7},
pages = {1319--27},
pmid = {22645380},
title = {{RNASTAR: An RNA STructural Alignment Repository that provides insight into the evolution of natural and artificial RNAs.}},
