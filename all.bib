journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Fungal Proteins,Fungal Proteins: metabolism,Genome, Fungal,Protein Binding,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Saccharomyces cerevisiae: metabolism,Two-Hybrid System Techniques},
month = apr,
number = {8},
pages = {4569--74},
pmid = {11283351},
title = {{A comprehensive two-hybrid analysis to explore the yeast protein interactome.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=31875\&tool=pmcentrez\&rendertype=abstract},
volume = {98},
