journal = {Journal of bacteriology},
keywords = {Amino Acids,Amino Acids: metabolism,Anions,Bacterial Proteins,Bacterial Proteins: biosynthesis,Chlorides,Chlorides: metabolism,Chlorides: pharmacology,Glutamates,Glutamates: pharmacology,Glutamic Acid,Halobacterium,Halobacterium: metabolism,Osmolar Concentration,Phenylalanine-tRNA Ligase,Phenylalanine-tRNA Ligase: metabolism,Sodium Chloride,Sodium Chloride: pharmacology,Vibrio,Vibrio: drug effects,Vibrio: metabolism},
month = oct,
number = {1},
pages = {385--90},
pmid = {6148335},
title = {{Effect of chloride and glutamate ions on in vitro protein synthesis by the moderate halophile Vibrio costicola.}},
