pmid = {21138969},
title = {{Engineering cooperative tecto-RNA complexes having programmable stoichiometries.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3074147\&tool=pmcentrez\&rendertype=abstract},
volume = {39},
year = {2011}
}
