keywords = {Animals,Base Pairing,Binding Sites,Cations, Divalent,Cations, Divalent: chemistry,Cations, Monovalent,Cations, Monovalent: chemistry,Cesium,Cesium: chemistry,Cobalt,Cobalt: chemistry,Crystallization,Crystallography, X-Ray,Energy Transfer,Magnesium,Magnesium: chemistry,Metals,Metals: chemistry,Potassium,Potassium: chemistry,Protein Structure, Secondary,Protein Structure, Tertiary,RNA, Bacterial,RNA, Ribosomal,RNA, Ribosomal: chemistry,Signal Recognition Particle,Signal Recognition Particle: chemistry,Tetrahymena thermophila,Tetrahymena thermophila: chemistry},
