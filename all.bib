pages = {8434--8443},
pmid = {23868089},
title = {{Evidence of direct complementary interactions between messenger RNAs and their cognate proteins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23868089},
volume = {41},
year = {2013}
}
