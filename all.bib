keywords = {Animals,Crystallography, X-Ray,Crystallography, X-Ray: standards,Databases, Protein,Humans,Models, Molecular,Molecular Structure,Protein Conformation,Proteins,Proteins: chemistry,X-Ray Diffraction},
month = jan,
number = {1},
