file = {:Users/bsweene/Documents/Papers/International Journal of Systematic and Evolutionary Microbiology/2000/Arthrobacter flavus sp. nov., a psychrophilic bacterium isolated from a pond in McMurdo Dry Valley, Antarctica.pdf:pdf},
