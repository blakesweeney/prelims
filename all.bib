journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {Cold Temperature,DNA, Bacterial,DNA, Bacterial: genetics,DNA, Ribosomal,DNA, Ribosomal: genetics,Gammaproteobacteria,Gammaproteobacteria: classification,Gammaproteobacteria: genetics,Gammaproteobacteria: isolation \& purification,Gammaproteobacteria: physiology,Halomonas,Halomonas: classification,Halomonas: genetics,Halomonas: isolation \& purification,Japan,Moraxella,Moraxella: classification,Moraxella: genetics,Moraxella: isolation \& purification,Movement,Nucleic Acid Hybridization,Phenotype,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Seawater,Seawater: microbiology,Sequence Analysis, DNA,Water Microbiology},
