publisher = {Nature Publishing Group},
title = {{Should we stay or should we go: mechanisms and ecological consequences for biofilm dispersal.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22120588},
volume = {10},
year = {2012}
}
