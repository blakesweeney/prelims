@article{Grabow2011,
abstract = {RNA is an attractive biopolymer for nanodesign of self-assembling particles for nanobiotechnology and synthetic biology. Here, we experimentally characterize by biochemical and biophysical methods the formation of thermostable and ribonuclease resistant RNA nanorings previously proposed by computational design. High yields of fully programmable nanorings were produced based on several RNAI/IIi kissing complex variants selected for their ability to promote polygon self-assembly. This self-assembly strategy relying on the particular geometry of bended kissing complexes has potential for developing short interfering RNA delivery agents.},
