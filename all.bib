month = oct,
number = {10},
pages = {1875--85},
pmid = {19710185},
title = {{New metrics for comparing and assessing discrepancies between RNA 3D structures and models.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2743038\&tool=pmcentrez\&rendertype=abstract},
volume = {15},
