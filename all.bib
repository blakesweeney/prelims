month = jul,
pages = {1--17},
pmid = {23847102},
title = {{Widespread purifying selection on RNA structure in mammals.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23847102},
year = {2013}
}
@article{Rahrig2010,
abstract = {Comparing 3D structures of homologous RNA molecules yields information about sequence and structural variability. To compare large RNA 3D structures, accurate automatic comparison tools are needed. In this article, we introduce a new algorithm and web server to align large homologous RNA structures nucleotide by nucleotide using local superpositions that accommodate the flexibility of RNA molecules. Local alignments are merged to form a global alignment by employing a maximum clique algorithm on a specially defined graph that we call the 'local alignment' graph.},
