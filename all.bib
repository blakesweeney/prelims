issn = {1476-4687},
journal = {Nature},
keywords = {Adaptation, Physiological,Adaptation, Physiological: radiation effects,Biological Evolution,Cyanobacteria,Cyanobacteria: classification,Cyanobacteria: genetics,Cyanobacteria: radiation effects,Environment,Genes, Bacterial,Genes, Bacterial: genetics,Genome, Bacterial,Light,Molecular Sequence Data,Oceans and Seas,Phylogeny},
month = aug,
