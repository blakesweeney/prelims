journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {16S,16S: genetics,23S,23S: genetics,Anaerobiosis,Bacteria,Bacteria: classification,Bacteria: genetics,Bacteria: isolation \& purification,Bacteria: metabolism,Bacterial,Bacterial: genetics,DNA,Extraction and Processing Industry,Heterotrophic Processes,Hot Temperature,Molecular Sequence Data,Phylogeny,RNA,Ribosomal,Ribosomal: genetics,Seawater,Seawater: microbiology,Sulfates,Sulfates: metabolism},
