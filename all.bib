title = {{Statistical evaluation of improvement in RNA secondary structure prediction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22139940},
volume = {40},
year = {2012}
}
@article{Wadley2004b,
abstract = {Recurring RNA structural motifs are important sites of tertiary interaction and as such, are integral to RNA macromolecular structure. Although numerous RNA motifs have been classified and characterized, the identification of new motifs is of great interest. In this study, we discovered four new conformationally recurring motifs: the pi-turn, the Omega-turn, the alpha-loop and the C2'-endo mediated flipped adenosine motif. Not only do they have complex and interesting structures, but they participate in contacts of high biological significance. In a first for the RNA field, new motifs were discovered by a fully automated algorithm. This algorithm, COMPADRES, utilized a reduced representation of the RNA backbone and was highly successful at discerning unique structural relationships. This study also shows that recurring RNA substructures are not necessarily accompanied by consistent primary or secondary structure.},
author = {Wadley, Leven M and Pyle, Anna Marie},
doi = {10.1093/nar/gkh1002},
file = {:Users/bsweene/Documents/Papers/Nucleic acids research/2004/The identification of novel RNA structural motifs using COMPADRES an automated approach to structural discovery(2).pdf:pdf},
issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Adenosine,Adenosine: chemistry,Algorithms,Hydrogen Bonding,Models,Molecular,Nucleic Acid Conformation,RNA,RNA: chemistry,Software,alpha-loop,omega-turn,pi-turn},
mendeley-tags = {alpha-loop,omega-turn,pi-turn},
