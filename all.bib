journal = {Current biology},
keywords = {Adaptation, Biological,Animals,Binding Sites,Computational Biology,Evolution, Molecular,Genes,Genes: physiology,Genome, Human,Humans,Proteins,Proteins: genetics,Proteins: physiology},
