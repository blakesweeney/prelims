issn = {1466-5026},
journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {Bacterial Typing Techniques,China,Cold Temperature,DNA, Bacterial,DNA, Bacterial: analysis,DNA, Ribosomal,DNA, Ribosomal: analysis,Genes, rRNA,Geologic Sediments,Geologic Sediments: microbiology,Molecular Sequence Data,Nucleic Acid Hybridization,Phenotype,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Seawater,Seawater: microbiology,Sequence Analysis, DNA,Shewanella,Shewanella: classification,Shewanella: genetics,Shewanella: isolation \& purification,Shewanella: physiology},
