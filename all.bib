issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Base Pairing,Cluster Analysis,Hydrogen Bonding,Models, Molecular,Nucleotide Motifs,RNA,RNA, Ribosomal,RNA, Ribosomal: chemistry,RNA: chemistry},
month = feb,
