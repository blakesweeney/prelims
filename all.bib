issn = {1431-0651},
journal = {Extremophiles},
keywords = {Antarctic Regions,Base Sequence,DNA, Bacterial,DNA, Bacterial: genetics,DNA, Ribosomal,DNA, Ribosomal: genetics,Fatty Acids,Fatty Acids: analysis,Fatty Acids: chemistry,Halomonas,Halomonas: classification,Halomonas: genetics,Halomonas: growth \& development,Halomonas: isolation \& purification,Ice,Molecular Sequence Data,Phenotype,Phylogeny,RNA, Bacterial,RNA, Bacterial: genetics,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Species Specificity},
