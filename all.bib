issn = {1431-0651},
journal = {Extremophiles},
keywords = {Antarctic Regions,Chlorella,Chlorella: classification,Chlorella: growth \& development,Chlorella: physiology,Chlorella: ultrastructure,Chlorophyta,Chlorophyta: classification,Chlorophyta: isolation \& purification,Chlorophyta: physiology,Cold Climate,Ecosystem,Electron Transport,Fatty Acids,Fatty Acids: analysis,Geography,Photochemistry,Phylogeny,Pigments, Biological,Pigments, Biological: isolation \& purification},
