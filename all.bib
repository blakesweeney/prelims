journal = {Environmental microbiology},
keywords = {Animals,Anthozoa,Anthozoa: microbiology,Antibiosis,Bacteria,Bacteria: genetics,Bacteria: growth \& development,Bacteria: isolation \& purification,DNA, Bacterial,DNA, Bacterial: genetics,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Sequence Analysis, DNA,Temperature,Water Microbiology},
month = jan,
number = {1},
