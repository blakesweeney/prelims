pmid = {21715380},
title = {{Exploiting Oxytricha trifallax nanochromosomes to screen for non-coding RNA genes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3177221\&tool=pmcentrez\&rendertype=abstract},
