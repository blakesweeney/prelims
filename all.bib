journal = {PloS one},
keywords = {Algorithms,Base Pairing,Computational Biology,Computational Biology: methods,Nucleic Acid Conformation,RNA, Bacterial,RNA, Bacterial: chemistry,RNA, Bacterial: genetics,RNA, Ribosomal,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: chemistry,RNA, Ribosomal, 16S: genetics,RNA, Ribosomal, 23S,RNA, Ribosomal, 23S: chemistry,RNA, Ribosomal, 23S: genetics,RNA, Ribosomal, 5S,RNA, Ribosomal, 5S: chemistry,RNA, Ribosomal, 5S: genetics,RNA, Ribosomal: chemistry,RNA, Ribosomal: genetics},
month = jan,
