journal = {Bioinformatics},
keywords = {16S,16S: chemistry,Algorithms,Nucleic Acid Conformation,RNA,RNA: chemistry,RNA: methods,Ribosomal,Sequence Alignment,Sequence Alignment: methods,Sequence Analysis,Software},
month = nov,
