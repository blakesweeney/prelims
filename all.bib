journal = {Journal of molecular biology},
keywords = {Base Sequence,Binding Sites,Hydroxyl Radical,Hydroxyl Radical: chemistry,Molecular Sequence Data,Nucleic Acid Conformation,Phosphates,Phosphates: chemistry,Phosphates: metabolism,Potassium,Potassium: chemistry,Potassium: metabolism,RNA, Ribosomal, 23S,RNA, Ribosomal, 23S: chemistry,RNA, Ribosomal, 23S: metabolism,Ribosomal Proteins,Ribosomal Proteins: chemistry,Ribosomal Proteins: metabolism,Thermodynamics},
month = may,
number = {4},
pages = {963--73},
pmid = {12054794},
title = {{A compact RNA tertiary structure contains a buried backbone-K+ complex.}},
