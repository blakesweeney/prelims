journal = {Annual review of biophysics},
keywords = {Bacteria,Bacteria: chemistry,Bacteria: metabolism,Crystallography, X-Ray,Kinetics,Models, Molecular,Physicochemical Phenomena,Protein Biosynthesis,Ribosomal Proteins,Ribosomal Proteins: metabolism,Ribosomes,Ribosomes: chemistry,Ribosomes: genetics,Ribosomes: metabolism,Thermodynamics},
