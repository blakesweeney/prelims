pages = {4578--83},
pmid = {16093549},
title = {{Computer identification of snoRNA genes using a Mammalian Orthologous Intron Database.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1184218\&tool=pmcentrez\&rendertype=abstract},
