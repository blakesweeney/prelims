@article{Nolivos2005,
abstract = {The C/D guide RNAs predicted from the genomic sequences of three species of Pyrococcus delineate a family of small non-coding archaeal RNAs involved in the methylation of rRNA and tRNA. The C/D guides assemble into ribonucleoprotein (RNP) that contains the methyltransferase. The protein L7Ae, a key structural component of the RNP, binds to a Kink-turn (K-turn) formed by the C/D motif. The K-turn is a structure that consists of two RNA stems separated by a short asymmetric loop with a characteristic sharp bend (kink) between the two stems. The majority of the pyrococcal C/D guides contain a short 3 nt-spacer between the C'/D' motifs. We show here that conserved terminal stem-loops formed by the C'/D' motif of the Pyrococcus C/D RNAs are also L7Ae-binding sites. These stem-loops are related to the K-turn by sequence and structure, but they consist of a single stem closed by a terminal loop. We have named this structure the K-loop. We show that conserved non-canonical base pairs in the stem of the K-loop are necessary for L7Ae binding. For the C/D guides with a 3 nt-spacer we show that the sequence and length is also important. The K-loop could improve the stability of the C/D guide RNAs in Pyrococcal species, which are extreme hyperthermophiles.},
author = {Nolivos, Sophie and Carpousis, Agamemnon J and Clouet-d'Orval, B\'{e}atrice},
doi = {10.1093/nar/gki962},
file = {::},
issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Archaeal Proteins,Archaeal Proteins: metabolism,Base Sequence,Binding Sites,Conserved Sequence,Molecular Sequence Data,Nucleic Acid Conformation,Pyrococcus,Pyrococcus: genetics,RNA, Archaeal,RNA, Archaeal: chemistry,RNA, Archaeal: metabolism,RNA, Guide,RNA, Guide: chemistry,RNA, Guide: metabolism,Ribonucleoproteins,Ribonucleoproteins: metabolism},
month = jan,
number = {20},
pages = {6507--14},
pmid = {16293637},
title = {{The K-loop, a general feature of the Pyrococcus C/D guide RNAs, is an RNA structural motif related to the K-turn.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1289080\&tool=pmcentrez\&rendertype=abstract},
volume = {33},
year = {2005}
}
