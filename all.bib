month = jul,
number = {7151},
pages = {338--41},
pmid = {17637666},
title = {{A reversible wet/dry adhesive inspired by mussels and geckos.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17637666},
volume = {448},
year = {2007}
}
