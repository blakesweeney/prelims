volume = {325},
year = {2009}
}
@article{Hall-Stoodley2005,
abstract = {Several pathogenic bacterial species that are found in the environment can form complex multicellular structures on surfaces known as biofilms. Pseudomonas aeruginosa, Vibrio cholerae and certain species of nontuberculous mycobacteria are examples of human pathogens that form biofilms in natural aquatic environments. We suggest that the dynamics of biofilm formation facilitates the transmission of pathogens by providing a stable protective environment and acting as a nidus for the dissemination of large numbers of microorganisms; both as detached biofilm clumps and by the fluid-driven dispersal of biofilm clusters along surfaces. We also suggest that emerging evidence indicates that biofilm formation conveys a selective advantage to certain pathogens by increasing their ability to persist under diverse environmental conditions.},
