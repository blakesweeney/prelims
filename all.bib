journal = {Methods},
keywords = {Algorithms,Automatic Data Processing,Calorimetry,Calorimetry: instrumentation,Calorimetry: methods,Nucleic Acid Conformation,Protein Binding,Protein Binding: genetics,RNA,RNA, Double-Stranded,RNA, Double-Stranded: chemistry,RNA: chemistry,RNA: metabolism,Thermodynamics,Titrimetry,Titrimetry: methods},
month = mar,
