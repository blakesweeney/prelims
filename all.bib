pmid = {21124488},
publisher = {Nature Publishing Group},
title = {{An integrative study of a meromictic lake ecosystem in Antarctica.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3105772\&tool=pmcentrez\&rendertype=abstract},
volume = {5},
