journal = {Cold Spring Harbor symposia on quantitative biology},
keywords = {Evolution, Molecular,Models, Molecular,Molecular Structure,Protein Biosynthesis,RNA,RNA: chemistry,RNA: genetics,RNA: metabolism,Ribosomes,Ribosomes: chemistry,Ribosomes: genetics,Ribosomes: metabolism},
