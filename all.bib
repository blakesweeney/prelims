journal = {Database : the journal of biological databases and curation},
keywords = {Database Management Systems,Databases,Enzymes,Enzymes: chemistry,Enzymes: genetics,Enzymes: metabolism,Humans,Mutation,Pharmaceutical Preparations,Pharmaceutical Preparations: chemistry,Pharmaceutical Preparations: metabolism,Protein,User-Computer Interface},
