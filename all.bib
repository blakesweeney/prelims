file = {:Users/bsweene/Documents/Papers/Systematic and applied microbiology/2010/Update of the All-Species Living Tree Project based on 16S and 23S rRNA sequence analyses.pdf:pdf},
issn = {1618-0984},
journal = {Systematic and applied microbiology},
keywords = {Archaea,Archaea: classification,Archaea: genetics,Bacteria,Bacteria: classification,Bacteria: genetics,Databases, Genetic,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,RNA, Ribosomal, 23S,RNA, Ribosomal, 23S: genetics},
