journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {Anaerobiosis,Bacterial Typing Techniques,Base Composition,Crenarchaeota,Crenarchaeota: classification,Crenarchaeota: genetics,Crenarchaeota: growth \& development,Crenarchaeota: ultrastructure,DNA, Archaeal,DNA, Archaeal: chemistry,DNA, Archaeal: genetics,DNA, Ribosomal,DNA, Ribosomal: chemistry,DNA, Ribosomal: genetics,Fresh Water,Fresh Water: microbiology,Hot Temperature,Hydrogen-Ion Concentration,Molecular Sequence Data,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Russia,Sequence Analysis, DNA},
