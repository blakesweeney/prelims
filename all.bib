title = {{The identification of novel RNA structural motifs using COMPADRES: an automated approach to structural discovery.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=545444\&tool=pmcentrez\&rendertype=abstract},
volume = {32},
year = {2004}
}
