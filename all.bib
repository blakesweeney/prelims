file = {:Users/bsweene/Documents/Papers/International journal of systematic bacteriology/1997/Psychroserpens burtonensis gen. nov., sp. nov., and Gelidibacter algens gen. nov., sp. nov., psychrophilic bacteria isolated from antarctic lacustrine and sea ice habitats.pdf:pdf},
issn = {0020-7713},
journal = {International journal of systematic bacteriology},
keywords = {Antarctic Regions,Cold Temperature,DNA, Bacterial,DNA, Bacterial: analysis,DNA, Ribosomal,DNA, Ribosomal: analysis,Genotype,Gram-Negative Bacteria,Gram-Negative Bacteria: classification,Gram-Negative Bacteria: genetics,Gram-Negative Bacteria: isolation \& purification,Molecular Sequence Data,Oceans and Seas,Phenotype,Phylogeny,Seawater,Seawater: microbiology},
