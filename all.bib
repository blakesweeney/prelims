journal = {Biophysical Journal},
keywords = {Amino Acids,Amino Acids: metabolism,Binding Sites,Biocatalysis,Conserved Sequence,Conserved Sequence: genetics,Enzyme Stability,Escherichia coli,Escherichia coli: metabolism,Guanosine Diphosphate,Guanosine Diphosphate: metabolism,Guanosine Triphosphate,Guanosine Triphosphate: metabolism,Hydrogen Bonding,Kinetics,Molecular Dynamics Simulation,Mutant Proteins,Mutant Proteins: chemistry,Mutant Proteins: metabolism,Mutation,Mutation: genetics,Peptide Elongation Factor Tu,Peptide Elongation Factor Tu: chemistry,Peptide Elongation Factor Tu: metabolism,Peptide Elongation Factors,Peptide Elongation Factors: metabolism,Protein Binding,Protein Conformation,Thermodynamics},
month = dec,
