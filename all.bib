journal = {The FEBS journal},
keywords = {16S,16S: chemistry,16S: genetics,16S: metabolism,Bacterial,Bacterial: genetics,Bacterial: metabolism,Mutagenesis,Mutation,Mutation: genetics,Nucleic Acid Conformation,Protein Biosynthesis,RNA,Ribosomal,Ribosomes,Ribosomes: chemistry,Ribosomes: metabolism},
