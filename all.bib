doi = {10.1099/ijs.0.65081-0},
file = {:Users/bsweene/Documents/Papers/International Journal of Systematic and Evolutionary Microbiology/2007/Reclassification of Vibrio fischeri, Vibrio logei, Vibrio salmonicida and Vibrio wodanis as Aliivibrio fischeri gen. nov., comb. nov., Aliivibrio logei comb. nov., Aliivibrio salmonicida comb. nov. and Aliivibrio wodanis comb. nov.pdf:pdf},
