journal = {Nature},
keywords = {Biological Transport,Cryoelectron Microscopy,Escherichia coli,Guanosine Triphosphate,Guanosine Triphosphate: metabolism,Hydrolysis,Macromolecular Substances,Models, Molecular,Molecular Conformation,Peptide Elongation Factor G,Peptide Elongation Factor G: metabolism,RNA, Messenger,RNA, Messenger: metabolism,RNA, Transfer,RNA, Transfer: metabolism,Ribosomes,Ribosomes: chemistry,Ribosomes: metabolism,Ribosomes: ultrastructure},
month = jul,
