keywords = {Animals,Bacteria,Bacteria: genetics,MicroRNAs,MicroRNAs: metabolism,Oxidative Stress,RNA, Small Interfering,RNA, Small Interfering: metabolism,RNA, Small Untranslated,RNA, Small Untranslated: metabolism,Systems Biology},
