journal = {Journal of virology},
keywords = {Base Pairing,Base Sequence,Molecular Sequence Data,Mutagenesis,Nucleic Acid Conformation,Plant Viruses,Plant Viruses: chemistry,Plant Viruses: genetics,Plant Viruses: metabolism,Protoplasts,Protoplasts: virology,RNA, Viral,RNA, Viral: chemistry,RNA, Viral: genetics,RNA, Viral: metabolism,Solanum tuberosum,Solanum tuberosum: virology,Tobacco,Tobacco: virology,Transcription, Genetic,Viroids,Viroids: chemistry,Viroids: metabolism,Viroids: pathogenicity,Viroids: physiology,Virus Replication},
month = sep,
number = {17},
