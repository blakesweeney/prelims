journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {Bacillus,Bacillus: growth \& development,Base Composition,Culture Media,DNA, Bacterial,DNA, Bacterial: chemistry,DNA, Bacterial: genetics,DNA, Ribosomal,DNA, Ribosomal: chemistry,DNA, Ribosomal: genetics,Gram-Negative Bacteria,Gram-Negative Bacteria: classification,Gram-Negative Bacteria: genetics,Gram-Negative Bacteria: growth \& development,Gram-Negative Bacteria: ultrastructure,Gram-Positive Asporogenous Rods,Gram-Positive Asporogenous Rods: classification,Gram-Positive Asporogenous Rods: genetics,Gram-Positive Asporogenous Rods: growth \& developm,Gram-Positive Asporogenous Rods: ultrastructure,Hot Temperature,Molecular Sequence Data,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Sequence Analysis, DNA,Soil Microbiology,Symbiosis,Temperature,Tryptophanase,Tryptophanase: metabolism},
