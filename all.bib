issn = {0962-8436},
journal = {Philosophical transactions of the Royal Society of London. Series B, Biological sciences},
keywords = {Acclimatization,Cold Temperature,Directed Molecular Evolution,Enzymes,Enzymes: chemistry,Enzymes: metabolism,Eukaryotic Cells,Eukaryotic Cells: enzymology,Eukaryotic Cells: metabolism,Kinetics,Prokaryotic Cells,Prokaryotic Cells: enzymology,Prokaryotic Cells: metabolism,Protein Folding,Structure-Activity Relationship},
month = jul,
