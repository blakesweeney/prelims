keywords = {Animals,Bacillus,Bacillus: classification,Bacillus: genetics,Bacillus: isolation \& purification,Base Sequence,DNA, Bacterial,Hot Temperature,Milk,Milk: microbiology,Molecular Sequence Data,Phenotype,Phylogeny,Spores, Bacterial,Spores, Bacterial: physiology},
month = jul,
