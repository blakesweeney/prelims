journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {3' Untranslated Regions,3' Untranslated Regions: genetics,5' Untranslated Regions,5' Untranslated Regions: genetics,Animals,Arabidopsis,Arabidopsis: genetics,Base Sequence,Binding Sites,Caenorhabditis elegans,Caenorhabditis elegans: genetics,Computational Biology,Computational Biology: methods,Drosophila melanogaster,Drosophila melanogaster: genetics,Humans,Introns,Introns: genetics,Monte Carlo Method,RNA Precursors,RNA Precursors: genetics,RNA Splicing,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Software,Transcription, Genetic},
