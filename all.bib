doi = {10.1099/ijs.0.63829-0},
file = {:Users/bsweene/Documents/Papers/International Journal of Systematic and Evolutionary Microbiology/2006/Shewanella halifaxensis sp. nov., a novel obligately respiratory and denitrifying psychrophile.pdf:pdf},
