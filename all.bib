keywords = {Azoarcus,Azoarcus: enzymology,Dose-Response Relationship, Drug,Magnesium Chloride,Magnesium Chloride: pharmacology,Nucleic Acid Conformation,Nucleic Acid Conformation: drug effects,Polyethylene Glycols,Polyethylene Glycols: pharmacology,RNA Stability,RNA Stability: drug effects,RNA, Catalytic,RNA, Catalytic: chemistry,Scattering, Small Angle,Water,Water: chemistry,X-Ray Diffraction},
month = jun,
number = {25},
pages = {8690--6},
pmid = {20521820},
title = {{Molecular crowding stabilizes folded RNA structure by the excluded volume effect.}},
