@article{Braun2012,
abstract = {Mutational events as well as the selection of the optimal variant are essential steps in the evolution of living organisms. The same principle is used in laboratory to extend the natural biodiversity to obtain better catalysts for applications in biomanufacturing or for improved biopharmaceuticals. Furthermore, single mutation in genes of drug-metabolizing enzymes can also result in dramatic changes in pharmacokinetics. These changes are a major cause of patient-specific drug responses and are, therefore, the molecular basis for personalized medicine. MuteinDB systematically links laboratory-generated enzyme variants (muteins) and natural isoforms with their biochemical properties including kinetic data of catalyzed reactions. Detailed information about kinetic characteristics of muteins is available in a systematic way and searchable for known mutations and catalyzed reactions as well as their substrates and known products. MuteinDB is broadly applicable to any known protein and their variants and makes mutagenesis and biochemical data searchable and comparable in a simple and easy-to-use manner. For the import of new mutein data, a simple, standardized, spreadsheet-based data format has been defined. To demonstrate the broad applicability of the MuteinDB, first data sets have been incorporated for selected cytochrome P450 enzymes as well as for nitrilases and peroxidases. Database URL: http://www.MuteinDB.org.},
author = {Braun, Andreas and Halwachs, Bettina and Geier, Martina and Weinhandl, Katrin and Guggemos, Michael and Marienhagen, Jan and Ruff, Anna J and Schwaneberg, Ulrich and Rabin, Vincent and {Torres Pazmi\~{n}o}, Daniel E and Thallinger, Gerhard G and Glieder, Anton},
doi = {10.1093/database/bas028},
issn = {1758-0463},
journal = {Database},
keywords = {Database Management Systems,Databases, Protein,Enzymes,Enzymes: chemistry,Enzymes: genetics,Enzymes: metabolism,Humans,Mutation,Pharmaceutical Preparations,Pharmaceutical Preparations: chemistry,Pharmaceutical Preparations: metabolism,User-Computer Interface},
month = jan,
pages = {bas028},
pmid = {22730453},
title = {{MuteinDB: the mutein database linking substrates, products and enzymatic reactions directly with genetic variants of enzymes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3381223\&tool=pmcentrez\&rendertype=abstract},
volume = {2012},
year = {2012}
}
