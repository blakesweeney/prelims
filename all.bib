file = {:Users/bsweene/Documents/Papers/International Journal of Systematic and Evolutionary Microbiology/2007/Segetibacter koreensis gen. nov., sp. nov., a novel member of the phylum Bacteroidetes, isolated from the soil of a ginseng field in South Korea.pdf:pdf},
issn = {1466-5026},
journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {Bacteroidetes,Bacteroidetes: classification,Bacteroidetes: genetics,Bacteroidetes: isolation \& purification,DNA, Bacterial,DNA, Bacterial: genetics,DNA, Ribosomal,DNA, Ribosomal: genetics,Molecular Sequence Data,Panax,Panax: microbiology,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Soil Microbiology},
