pmid = {20484378},
title = {{BioCatalogue: a universal catalogue of web services for the life sciences.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2896129\&tool=pmcentrez\&rendertype=abstract},
