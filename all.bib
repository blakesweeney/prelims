file = {:Users/bsweene/Documents/Papers/Extremophiles/2006/Characterization of Exiguobacterium isolates from the Siberian permafrost. Description of Exiguobacterium sibiricum sp. nov.pdf:pdf},
issn = {1431-0651},
journal = {Extremophiles},
keywords = {Bacillaceae,Bacillaceae: classification,Bacillaceae: cytology,Bacillaceae: genetics,Bacillaceae: isolation \& purification,Ecosystem,Phylogeny,Siberia,Soil Microbiology},
month = aug,
number = {4},
pages = {285--94},
pmid = {16489412},
title = {{Characterization of Exiguobacterium isolates from the Siberian permafrost. Description of Exiguobacterium sibiricum sp. nov.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16489412},
