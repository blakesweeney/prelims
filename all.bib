journal = {Science},
keywords = {Alphaproteobacteria,Alphaproteobacteria: physiology,Archaea,Archaea: physiology,Bacterial Physiological Phenomena,Climate Change,Ecosystem,Models, Biological,Oceans and Seas,Phytoplankton,Phytoplankton: growth \& development,Phytoplankton: physiology,Seasons,Seawater,Seawater: chemistry,Seawater: microbiology,Temperature},
