pmid = {2563368},
title = {{In vitro protein synthesis by the moderate halophile Vibrio costicola: site of action of Cl- ions.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=209678\&tool=pmcentrez\&rendertype=abstract},
volume = {171},
year = {1989}
}
