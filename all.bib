@article{Calcagno2012,
abstract = {The study of science-making is a growing discipline that builds largely on online publication and citation databases, while prepublication processes remain hidden. Here, we report results from a large-scale survey of the submission process, covering 923 scientific journals from the biological sciences in years 2006-2008. Manuscript flows among journals revealed a modular submission network, with high-impact journals preferentially attracting submissions. However, about 75\% of published articles were submitted first to the journal that would publish them, and high-impact journals published proportionally more articles that had been resubmitted from another journal. Submission history affected postpublication impact: Resubmissions from other journals received significantly more citations than first-intent submissions, and resubmissions between different journal communities received significantly fewer citations.},
author = {Calcagno, V and Demoinet, E and Gollner, K and Guidi, L and Ruths, D and de Mazancourt, C},
doi = {10.1126/science.1227833},
issn = {1095-9203},
journal = {Science},
month = oct,
pmid = {23065906},
title = {{Flows of Research Manuscripts Among Scientific Journals Reveal Hidden Submission Patterns.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23065906},
volume = {1065},
year = {2012}
}
@article{Pedraza2008,
abstract = {Many cellular components are present in such low numbers per cell that random births and deaths of individual molecules can cause substantial "noise" in concentrations. But biochemical events do not necessarily occur in single steps of individual molecules. Some processes are greatly randomized when synthesis or degradation occurs in large bursts of many molecules during a short time interval. Conversely, each birth or death of a macromolecule could involve several small steps, creating a memory between individual events. We present a generalized theory for stochastic gene expression, formulating the variance in protein abundance in terms of the randomness of the individual gene expression events. We show that common types of molecular mechanisms can produce gestation and senescence periods that reduce noise without requiring higher abundances, shorter lifetimes, or any concentration-dependent control loops. We also show that most single-cell experimental methods cannot distinguish between qualitatively different stochastic principles, although this in turn makes such methods better suited for identifying which components introduce fluctuations. Characterizing the random events that give rise to noise in concentrations instead requires dynamic measurements with single-molecule resolution.},
author = {Pedraza, Juan M and Paulsson, Johan},
doi = {10.1126/science.1144331},
file = {:Users/bsweene/Documents/Papers/Science/2008/Effects of molecular memory and bursting on fluctuations in gene expression.pdf:pdf},
issn = {1095-9203},
journal = {Science},
keywords = {Gene Expression,Gene Regulatory Networks,Mathematics,Models, Genetic,Protein Biosynthesis,Proteins,Proteins: metabolism,RNA, Messenger,RNA, Messenger: genetics,RNA, Messenger: metabolism,Stochastic Processes,Transcription, Genetic},
month = jan,
number = {5861},
pages = {339--43},
pmid = {18202292},
title = {{Effects of molecular memory and bursting on fluctuations in gene expression.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18202292},
volume = {319},
year = {2008}
