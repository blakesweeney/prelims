month = jan,
number = {16},
pages = {4925--36},
pmid = {15448187},
title = {{A comparative method for finding and folding RNA secondary structures within protein-coding regions.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=519121\&tool=pmcentrez\&rendertype=abstract},
volume = {32},
