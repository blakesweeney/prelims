journal = {Computational biology and chemistry},
keywords = {Animals,Archaea,Bacteria,Base Composition,DNA,DNA: classification,DNA: genetics,Eukaryotic Cells,Fungi,Genome,Humans,Nucleotides,Nucleotides: chemistry,Nucleotides: genetics,Plasmids,Plasmids: genetics,Sequence Analysis,Statistics as Topic,Viruses},
