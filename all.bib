@article{Rodnina2011a,
abstract = {Translocation of tRNA and mRNA through the ribosome is one of the most dynamic events during protein synthesis. In the cell, translocation is catalysed by EF-G (elongation factor G) and driven by GTP hydrolysis. Major unresolved questions are: how the movement is induced and what the moving parts of the ribosome are. Recent progress in time-resolved cryoelectron microscopy revealed trajectories of tRNA movement through the ribosome. Driven by thermal fluctuations, the ribosome spontaneously samples a large number of conformational states. The spontaneous movement of tRNAs through the ribosome is loosely coupled to the motions within the ribosome. EF-G stabilizes conformational states prone to translocation and promotes a conformational rearrangement of the ribosome (unlocking) that accelerates the rate-limiting step of translocation: the movement of the tRNA anticodons on the small ribosomal subunit. EF-G acts as a Brownian ratchet providing directional bias for movement at the cost of GTP hydrolysis.},
author = {Rodnina, Marina V and Wintermeyer, Wolfgang},
doi = {10.1042/BST0390658},
file = {:Users/bsweene/Documents/Papers/Biochemical Society transactions/2011/The ribosome as a molecular machine the mechanism of tRNA-mRNA movement in translocation.pdf:pdf},
issn = {1470-8752},
journal = {Biochemical Society transactions},
keywords = {Animals,Biological Transport,Biological Transport: physiology,Humans,Models, Biological,Models, Molecular,Movement,Movement: physiology,Protein Biosynthesis,Protein Biosynthesis: physiology,RNA, Messenger,RNA, Messenger: metabolism,RNA, Messenger: physiology,RNA, Transfer,RNA, Transfer: metabolism,RNA, Transfer: physiology,Ribosomes,Ribosomes: metabolism,Ribosomes: physiology,Signal Transduction},
month = apr,
number = {2},
pages = {658--62},
pmid = {21428957},
title = {{The ribosome as a molecular machine: the mechanism of tRNA-mRNA movement in translocation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21428957},
volume = {39},
year = {2011}
}
