keywords = {Animals,Base Sequence,Humans,Models, Biological,Molecular Sequence Data,Nucleic Acid Conformation,Phylogeny,RNA,RNA: analysis,RNA: chemistry,Sequence Alignment,Sequence Alignment: methods,Sequence Alignment: trends,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Sequence Homology, Nucleic Acid,Software},
month = sep,
number = {9},
pages = {1623--31},
pmid = {19622678},
title = {{The RNA structure alignment ontology.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2743057\&tool=pmcentrez\&rendertype=abstract},
volume = {15},
year = {2009}
}
