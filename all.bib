keywords = {Algorithms,Artificial Intelligence,Base Sequence,Bayes Theorem,Computer Simulation,Conserved Sequence,Conserved Sequence: genetics,Evolution, Molecular,Models, Chemical,Models, Genetic,Models, Molecular,Molecular Sequence Data,Monte Carlo Method,Pattern Recognition, Automated,RNA,RNA: chemistry,RNA: genetics,RNA: ultrastructure,Sequence Alignment,Sequence Alignment: methods,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Software,Structure-Activity Relationship},
month = aug,
number = {8},
pages = {e149},
pmid = {17696604},
title = {{SimulFold: simultaneously inferring RNA structures including pseudoknots, alignments, and trees using a Bayesian MCMC framework.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1941756\&tool=pmcentrez\&rendertype=abstract},
