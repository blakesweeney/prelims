month = oct,
number = {18},
pages = {6274--85},
pmid = {20466811},
title = {{Structure of the K-turn U4 RNA: a combined NMR and SANS study.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2952850\&tool=pmcentrez\&rendertype=abstract},
volume = {38},
year = {2010}
}
